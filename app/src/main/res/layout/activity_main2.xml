<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bagas.myapplication.Main2Activity">


    <TextView
        android:id="@+id/text_judul"
        android:layout_width="156dp"
        android:layout_height="28dp"
        android:text="makan malam"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="44dp" />

    <TextView
        android:id="@+id/tempat_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text_judul"
        android:layout_centerHorizontal="true"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_menu"
        android:layout_width="156dp"
        android:layout_height="28dp"
        android:layout_centerHorizontal="true"
        android:text="menu"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="139dp" />

    <TextView
        android:id="@+id/menu_makanan_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text_menu"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />


    <TextView
        android:id="@+id/text_porsi"
        android:layout_width="156dp"
        android:layout_height="28dp"
        android:text="porsi makan"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="231dp" />

    <TextView
        android:id="@+id/porsi_makanan_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text_porsi"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />


    <TextView
android:id="@+id/text_harga"
android:layout_width="156dp"
android:layout_height="28dp"
android:text="harga"
tools:layout_editor_absoluteX="739dp"
tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/text_harga_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:droid:layout_below="@+id/text_harga"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    </android.support.constraint.ConstraintLayout>
